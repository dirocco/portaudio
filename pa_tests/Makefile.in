PREFIX = @prefix@
CC = @CC@
CFLAGS = @CFLAGS@ -I. -I../../../include 
CPPFLAGS=@CPPFLAGS@
LDFLAGS=@LDFLAGS@
LIBS = -lportaudio @LIBS@ 
AR = @AR@
RANLIB = @RANLIB@
INSTALL = @INSTALL@
SHARED_FLAGS = @SHARED_FLAGS@
DLL_LIBS = @DLL_LIBS@
TOOLROOT=@TOOLROOT@
LOCALOPT=@LOCALOPT@
LOCALLINK=@LOCALLINK@
LOCALDLINK=@LOCALDLINK@


SOURCES = \
	patest_buffer.c \
	patest_clip.c \
	patest_dither.c \
	patest_hang.c \
	patest_leftright.c \
	patest_longsine.c \
	patest_many.c \
	patest_maxsines.c \
	patest_multi_sine.c \
	patest_pink.c \
	patest_ringmix.c \
	patest_saw.c \
	patest_sine8.c \
	patest_sine.c \
	patest_sine_formats.c \
	patest_sine_time.c \
	patest_stop.c \
	patest_sync.c \
	patest_toomanysines.c \
	patest_underflow.c \
	patest_wire.c \

# removed since it doesn't work as expected on MacOSX
#	patest_latency.c \

# This test is generates warnings
#	patest_record.c \

EXECS = $(SOURCES:%.c=../bin/%@EXEEXT@)

all: $(EXECS)

$(EXECS): ../bin/%@EXEEXT@: Makefile %.c
	$(CC) -o $@ $(CFLAGS) $*.c -L../../../lib $(LIBS) $(LDFLAGS) $(LOCALOPT) $(LOCALLINK)

clean:
	echo No intermediate files here 

distclean: clean
	cd ../bin;$(RM) -f $(EXECS);cd ../pa_tests

%.@OBJEXT@: %.c Makefile $(PAINC)
	$(CC) -c $(CFLAGS) $< -o $@ $(LOCALOPT)
